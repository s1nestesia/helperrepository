<h1>Users</h1>

<table>
  <tbody>
    <% @users.each do |user| %>
      <% unless user.id == current_user.id %>      
        <tr class="user-all">
          <td class="user-all"><%= user.email %></td>
          <td class="user-all"><%= link_to 'Show', user_path(user) %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>